<ApplicationConfiguration>
	<PageTitle>Lussumo Filebrowser 1.3.3</PageTitle>
	<PageIntroduction>[strong]This is the Filebrowser.[/strong]
      It is another free product from Lussumo that you can use to easily browse directories on your website.
      You can download the latest version of this product at [a href="http://thefilebrowser.com"]thefilebrowser.com[/a]</PageIntroduction>
	<BrowseSubFolders>true</BrowseSubFolders>
	<DateFormat>Y-m-d</DateFormat>
	<DisplayHiddenFiles>false</DisplayHiddenFiles>
	<FilesPerPage>30</FilesPerPage>
	<FitImagesToPage>true</FitImagesToPage>
	<HideFiles>fb.php,thumbnailer.php,index.php,.htaccess,download.php,download.phps,du.txt,adsensetest.php,isaac.css,mirrors.php,slate.css,test.php,test2.php,robots.txt,archivecount.inc,archivesize.inc,CHANGELOG,FILES,ls-lR,evolt.ico,favicon.ico,foo,index.phps,mkarchivesize,beodl,mirrors,assets,oldbeo,tmp,readme.html,lastupdated.txt</HideFiles>
	<MaxFilesPerPage>60</MaxFilesPerPage>
	<MaxThumbHeight>100</MaxThumbHeight>
	<MaxThumbWidth>100</MaxThumbWidth>
	<PluginHeight>500</PluginHeight>
	<PluginWidth>500</PluginWidth>
	<SortBy>Name</SortBy>
	<SortDirection>asc</SortDirection>
	<StyleUrl>_default.css</StyleUrl>
	<ThumbnailerStyleUrl>_thumbnailer.css</ThumbnailerStyleUrl>
	<UsePageIntroductionInSubFolders>true</UsePageIntroductionInSubFolders>
	<UseThumbnails>true</UseThumbnails>
</ApplicationConfiguration>
